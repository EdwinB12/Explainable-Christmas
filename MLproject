name: explainable_salt

entry_points:
    run_all:
        command: "python Jobs/process_data.py && \
            python Jobs/train.py && \
            python Jobs/explain.py"

    data-train:
        command: "python Jobs/process_data.py && \
            python Jobs/train.py"

    train-explain:
        command: "python Jobs/train.py && \
            python Jobs/explain.py"

    shap:
        command: "python Jobs/explain.py"
